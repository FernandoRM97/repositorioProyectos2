<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<display-name>loginController</display-name>
		<servlet-name>loginController</servlet-name>

		<servlet-class>filmografia.controller.loginController</servlet-class>
	</servlet>
	<servlet>
		<display-name>registrarController</display-name>
		<servlet-name>registrarController</servlet-name>
		<servlet-class>filmografia.controller.registrarController</servlet-class>
	</servlet>
	<servlet>
		<display-name>mantenimientoController</display-name>
		<servlet-name>mantenimientoController</servlet-name>
		<servlet-class>filmografia.controller.mantenimientoController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginController</servlet-name>
		<url-pattern>/loginController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>registrarController</servlet-name>
		<url-pattern>/registrarController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>mantenimientoController</servlet-name>
		<url-pattern>/mantenimientoController</url-pattern>
	</servlet-mapping>
</web-app>
